   "source": [
    "from datetime import date\n",
    "\n",
    "class Solution:\n",
    "    def dayOfTheWeek(self, day: int, month: int, year: int) -> str:\n",
    "        my_date = date(year, month, day)\n",
    "\n",
    "        return my_date.strftime(\"%A\")"
   ]
  }
