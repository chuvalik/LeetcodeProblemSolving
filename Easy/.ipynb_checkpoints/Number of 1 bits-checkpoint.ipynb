   "source": [
    "class Solution:\n",
    "    def hammingWeight(self, n: int) -> int:\n",
    "        count = 0\n",
    "\n",
    "        while n != 0:\n",
    "            count += n % 2\n",
    "            n = n // 2\n",
    "\n",
    "        return count"
   ]
  }
