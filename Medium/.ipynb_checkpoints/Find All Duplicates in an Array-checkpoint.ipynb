   "source": [
    "class Solution:\n",
    "    def findDuplicates(self, nums: List[int]) -> List[int]:\n",
    "        res = []\n",
    "\n",
    "        for num in nums:\n",
    "            index = abs(num) - 1\n",
    "            if nums[index] < 0: res.append(abs(num))\n",
    "            else: nums[index] = -nums[index]\n",
    "\n",
    "        return res\n"
   ]
  }
